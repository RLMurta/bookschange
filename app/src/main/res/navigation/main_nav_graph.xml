<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.compasso.bookschange.view.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_bookSearchFragment"
            app:destination="@id/bookSearchFragment" />
    </fragment>
    <fragment
        android:id="@+id/mapsFragment"
        android:name="com.compasso.bookschange.view.maps.MapsFragment"
        android:label="fragment_maps"
        tools:layout="@layout/fragment_maps" />
    <fragment
        android:id="@+id/bookSearchFragment"
        android:name="com.compasso.bookschange.view.home.bookSearch.BookSearchFragment"
        android:label="fragment_book_search"
        tools:layout="@layout/fragment_book_search" >
        <argument
            android:name="receive_database_name"
            app:argType="string" />
    </fragment>
</navigation>